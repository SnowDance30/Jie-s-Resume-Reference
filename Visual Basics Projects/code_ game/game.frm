Private Sub Form_Load()

Randomize Timer

assassin = Rnd * 24
If assassin = 0 Then assassin = 24
box(assassin).BackColor = vbBlack

For X = 0 To 24
    If X = assassin Then
    Else:
        Do
            a = Rnd * 3
            k(a) = k(a) + 1
            If k(0) > 9 Or k(1) > 8 Or k(2) > 7 Then k(a) = k(a) - 1
        Loop While k(0) < 10 And k(1) < 9 And k(2) < 8
        If a = 3 Then
            a = 0
            b = vbRed
        ElseIf a = 1 Then
            b = vbBlue
        Else
            b = vbYellow
        End If
        box(X).BackColor = b
        
    End If
Next

text.text = assassin


End Sub


